<%= simple_form_for @club do |f| %>
  <%= f.input :name %>
  <%= f.input :street %>
  <%= f.input :city %>
  <%= f.input :postcode %>
  <%= f.input :country %>
  <%= f.input :street_no %>
  <%= f.input :description %>
  <%= f.input :user_id, :as => :hidden, input_html: { value: current_user.id } %>
  <%= f.button :submit, "Create Club" %>
<% end %>
